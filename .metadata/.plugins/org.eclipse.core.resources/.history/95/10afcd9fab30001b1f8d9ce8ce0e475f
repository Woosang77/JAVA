package Practice.Lesson.ThreadPart.Function.E04_wait_notify;

public class DataBox {
	private String data;
	
	public String getData() {
		if(data == null) {
			try {
				wait();
			} catch (Exception e) {}
		}
		String getValue = data;
		System.out.println("ConsumerThread가 가져온 데이터 :" + getValue);
		data = null;
		notify();
		return getValue;
	}
	
	public String setData() {
		
	}
	
}
