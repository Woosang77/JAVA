package Practice.Lesson;

import java.awt.Toolkit;

public class BeepPrintExample1 {

	public static void main(String[] args) {
		//비프음을 5번 반복해서 소리나게하는 작업
		Toolkit toolkit = Toolkit.getDefaultToolkit();
		for(int i = 0; i < 5; i++) {
			toolkit.beep();
			try {
				Thread.sleep(500);
			} catch (Exception e) {
				// TODO: handle exception
			}
		}
		//"띵"문자열을 5번 출력하는 작업
	}

}
